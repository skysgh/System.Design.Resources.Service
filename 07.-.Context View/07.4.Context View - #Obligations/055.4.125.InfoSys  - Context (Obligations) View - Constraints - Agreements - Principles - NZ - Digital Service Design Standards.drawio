        </mxCell>
        <mxCell id="BPDDqp0WyomeCQy9vxMp-17" value="&lt;span style=&quot;white-space: nowrap&quot;&gt;Collect PII only if necessary, lawful and for a purpose connected to an agency.&lt;/span&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="VuUZPpVmZ8weyxiiEjUZ-1">
