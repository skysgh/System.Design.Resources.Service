        <mxCell id="BPDDqp0WyomeCQy9vxMp-22" value="&lt;span style=&quot;color: rgb(51 , 51 , 51) ; text-align: center&quot;&gt;Provide a way to challenge/correct own data.&lt;/span&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="VuUZPpVmZ8weyxiiEjUZ-1">
