        <mxCell id="BPDDqp0WyomeCQy9vxMp-20" value="&lt;span style=&quot;white-space: nowrap&quot;&gt;PII must be collectd lawfully&lt;/span&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="VuUZPpVmZ8weyxiiEjUZ-1">
